program test301
	const int jedan = 1;
	const int pet = 5;

	int niz[];
	
	char nizch[];
	
{
	void main() int bodovi;
	{
		
		bodovi = 0;
		bodovi++;  
		bodovi = bodovi + jedan;
		bodovi+= bodovi *= pet;
		bodovi--;
		print(bodovi);  // OUT: 11
			
		
		niz = new int[2];
		niz[0] = pet;
		niz[1] = 10;
		bodovi = (niz[1]/=niz[0]);
		print(bodovi); // OUT: 2
		print(niz[1]); // OUT: 2
		
		
		nizch = new char[3];
		nizch[0] = 'a';
		nizch[jedan] = 'b';
		nizch[pet - 3] = 'c';
		print( nizch[2 - 1]); // OUT: b
		print( nizch[jedan * 2]); // OUT: c
			
		
		bodovi += ((pet * jedan - 1) * 2 - (3 % 2 + 3 * 2 - 3) - 1 * 0); 
		print(bodovi); // OUT 6
			
			
		read(bodovi);
		bodovi = bodovi + 1;
		print(bodovi);
			
	
	}
}


